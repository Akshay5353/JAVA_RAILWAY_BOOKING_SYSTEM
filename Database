CREATE DATABASE RAILWAY;
USE RAILWAY; 
CREATE TABLE DETAILS (TNO INT PRIMARY KEY, TNAME VARCHAR (100), TSTART VARCHAR (100), TDESTINATION VARCHAR(100),TPRICE INT);
select * from Details;

CREATE TABLE RESERVATION (/*PID INT,foreign key(PID) references REGISTRATION(PID),*/PNO INT PRIMARY KEY,START_PLACE VARCHAR (100),END_PLACE VARCHAR (100),TRAIN_NO INT,TRAIN_NAME VARCHAR (100), PRICE INT, BDATE VARCHAR (100), NO_OF_TICKETS INT,TOTAL INT); 
SELECT * FROM RESERVATION; 
DROP TABLE RESERVATION;

CREATE TABLE REGISTRATION (PID INT PRIMARY KEY AUTO_INCREMENT,USERID VARCHAR (100) NOT NULL UNIQUE KEY, RPASSWORD VARCHAR (100) UNIQUE KEY NOT NULL,MAILID VARCHAR (100)UNIQUE KEY, HQUE VARCHAR (100), HANS VARCHAR (100), FNAME VARCHAR (100), LNAME VARCHAR (100), DOB varchar(50), ADDRESS VARCHAR (200), CITY VARCHAR (100), PIN INT, STATE VARCHAR (100), COUNTRY VARCHAR (100));
select * from registration;
DROP TABLE registration;

Create table LADMIN(AUDERID VARCHAR (100) NOT NULL UNIQUE KEY,APASSWORD VARCHAR (100) NOT NULL UNIQUE KEY);
INSERT INTO LADMIN VALUES ('ADMIN',1000), ('ADMIN2',1001),('ADMIN3',1002);
SELECT * FROM LADMIN;


