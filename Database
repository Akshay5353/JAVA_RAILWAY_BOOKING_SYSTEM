CREATE DATABASE RAILWAY;
USE RAILWAY; 
CREATE TABLE DETAILS (TNO INT PRIMARY KEY, TNAME VARCHAR (100), TSTART VARCHAR (100), TDESTINATION VARCHAR(100),TPRICE INT);
select * from Details;

CREATE TABLE RESERVATION (PNO INT PRIMARY KEY,START_PLACE VARCHAR (100),END_PLACE VARCHAR (100),TRAIN_NO INT,TRAIN_NAME VARCHAR (100), PRICE INT, BDATE VARCHAR (100), NO_OF_TICKETS INT,TOTAL INT); 

SELECT * FROM RESERVATION; 
DROP TABLE RESERVATION;
